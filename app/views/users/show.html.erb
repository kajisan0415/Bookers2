<%= render 'users/list',user: @user,book: @book %>

<body>
 <h1>Books</h1>
  <table>
   <thead>
   <th>Title</th>
   <th>Body</th>
   <th colspan="3"></th>
   </thead>
   <tbody>
    <% @books.each do |book| %>
    <tr>
       <%= attachment_image_tag @user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td>
       <%= link_to "show","/books/#{book.id}" %>
      </td>
    </tr>
    <% end %>
   </tbody>
  </table>
 
 </body>
