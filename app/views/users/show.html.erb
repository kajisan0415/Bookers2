<%= render 'users/list',users:@users %>

<body>
 <h1>Books</h1>
  <table>
   <thead>
   <th>Title</th>
   <th>Body</th>
   <th colspan="3"></th>
   </thead>
  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td>
       <%= link_to "show","/books/#{book.id}" %>
      </td>
    </tr>
     <% end %>
     <%= paginate @books %>
   </tbody>
  </table>
  
 </body>
